{
  "name": "gcf",
  "version": "1.0.0",
  "description": "Serverless functions for REACH app",
  "main": "index.js",
  "repository": "git@github.com:bettersg/reach-app.git",
  "author": "Oliver",
  "license": "MIT",
  "scripts": {
    "lint-staged": "lint-staged",
    "lint:fix": "eslint '{src,test}/**/*.{js,ts}' --quiet --fix",
    "build": "run(){ scripts/apply_project_config.sh $1 && rm -rf ./lib && BUILD_ENV=\"$1\" webpack && BUILD_ENV=\"$1\" node lib/prodConfigCheck.js; }; run",
    "deploy": "run(){ yarn build $1 && GOOGLE_APPLICATION_CREDENTIALS=.$1-key.json firebase deploy -P $1 --only functions:$2; }; run",
    "deployAll": "run(){ yarn build $1 && node lib/deploy.js && chmod 775 scripts/firebase_deploy.sh && scripts/ask_for_confirmation.sh && GOOGLE_APPLICATION_CREDENTIALS=.$1-key.json scripts/firebase_deploy.sh $1; }; run",
    "file": "run() { scripts/apply_project_config.sh $1 && GOOGLE_APPLICATION_CREDENTIALS=.$1-key.json PRETTY_LOGS=true ts-node -r tsconfig-paths/register $2 $3 $4; }; run",
    "testBase": "scripts/apply_project_config.sh stg && GOOGLE_APPLICATION_CREDENTIALS=.stg-key.json BUILD_ENV=testing PRETTY_LOGS=true mocha --recursive --timeout 8000 --exit -r ts-node/register --parallel --jobs 3 --paths -r tsconfig-paths/register -r @tt/shared/testUtils/hooks.ts",
    "test:unit": "nyc --reporter=html --reporter=json-summary --reporter=text --check-coverage --lines 60 yarn testBase src/**/*.test.ts --exclude src/**/*.integration.test.ts --exclude src/**/*.postDeployment.test.ts",
    "test:integration": "yarn testBase src/**/*.integration.test.ts",
    "test": "yarn test:unit && yarn test:integration",
    "prepare": "scripts/apply_project_config.sh stg"
  },
  "engines": {
      "node": "14"
  },
  "dependencies": {
    "@google-cloud/firestore": "^4.9.8",
    "@google-cloud/pubsub": "^2.13.0",
    "@google-cloud/secret-manager": "^3.2.0",
    "@google-cloud/storage": "^5.8.5",
    "@google-cloud/trace-agent": "^5.1.1",
    "@sentry/serverless": "^6.12.0",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/module-alias": "^2.0.1",
    "@types/nodemailer": "^6.4.4",
    "@types/source-map-support": "^0.5.4",
    "@types/stack-trace": "^0.0.29",
    "ajv": "^8.6.2",
    "ajv-formats": "^2.1.1",
    "archiver": "^5.0.0",
    "archiver-zip-encrypted": "^1.0.8",
    "async-retry": "^1.3.1",
    "aws-sdk": "^2.669.0",
    "axios": "^0.21.1",
    "csv-parse": "^4.10.1",
    "firebase-admin": "9.11.1",
    "firebase-functions": "^3.13.2",
    "firebase-tools": "^9.18.0",
    "google-gax": "^2.12.0",
    "google-protobuf": "^3.13.0",
    "husky": "^7.0.1",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.5.0",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "mocha": "^9.0.1",
    "module-alias": "^2.2.2",
    "moment-timezone": "^0.5.33",
    "node-apex-api-security": "^1.0.11",
    "node-fetch": "^2.6.1",
    "node-jose": "^2.0.0",
    "nodemailer": "^6.4.16",
    "nonce": "^1.0.4",
    "source-map-support": "^0.5.19",
    "stack-trace": "^0.0.10",
    "string-hash": "^1.1.3",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.5",
    "uuid": "^8.2.0",
    "websocket-extensions": ">=0.1.4"
  },
  "devDependencies": {
    "@types/archiver": "^3.1.0",
    "@types/async-retry": "^1.4.2",
    "@types/chai": "^4.2.0",
    "@types/chai-as-promised": "^7.1.1",
    "@types/mocha": "^8.2.2",
    "@types/node-fetch": "^2.5.7",
    "@types/node-jose": "^1.1.4",
    "@types/sinon": "^9.0.4",
    "@types/sinon-chai": "^3.2.5",
    "@types/string-hash": "^1.1.1",
    "@types/unzipper": "^0.10.4",
    "@types/uuid": "^7.0.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-exclude": "^2.0.3",
    "firebase": "^7.24.0",
    "firebase-functions-test": "^0.2.1",
    "generate-package-json-webpack-plugin": "^2.3.0",
    "lint-staged": "^11.0.0",
    "nock": "^13.0.11",
    "nyc": "^15.1.0",
    "prettier": "2.3.1",
    "protoc": "^1.0.4",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.7.0",
    "terser-webpack-plugin": "^5.2.4",
    "ts-loader": "^9.2.5",
    "ts-mock-imports": "^1.3.0",
    "ts-node": "^9.0.0",
    "ts-protoc-gen": "^0.12.0",
    "unzipper": "^0.10.11",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
